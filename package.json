{
  "name": "rainbow-parrot-messenger",
  "version": "0.1.1",
  "description": "Полноценное SPA 'Веб-мессенджер', реализованное только на JavaScript, TypeScript.",
  "source": "src/pages/index.hbs",
  "scripts": {
    "dev": "parcel --no-cache && parcel watch ./*",
    "build": "npm run clean && parcel build --no-cache --no-source-maps",
    "start": "npm run build && npm run static-server",
    "static-server": "node server",
    "clean": "rimraf dist"
  },
  "engines": {
    "node": ">=16.15.0",
    "npm": ">=8.5.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evalekseev/middle.messenger.praktikum.yandex.git"
  },
  "author": "Alekseev Evgeniy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/evalekseev/middle.messenger.praktikum.yandex/issues"
  },
  "homepage": "https://github.com/evalekseev/middle.messenger.praktikum.yandex#readme",
  "devDependencies": {
    "@anikin/parcel-transformer-handlebars": "^1.0.9",
    "parcel": "^2.5.0",
    "postcss": "^8.4.14",
    "postcss-nested": "^5.0.6",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "express": "^4.18.1"
  }
}
